<% provide(:title, "出勤中社員一覧") %>
<div>
  <h1>出勤中社員一覧</h1>
  <table class="table table-bordered table-condensed table-hover" id="table-attendances">
    <thead>
      <tr>
        <th>社員番号</th>
        <th>名前</th>
        
      </tr>
    </thead>
    <tbody>
      <% @in_attendances.each do |in_attendance| %>
      <tr>
        <td><%= in_attendance.user.employee_number %></td>
        <td><%= in_attendance.user.name %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>